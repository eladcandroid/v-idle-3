{
  "name": "v-idle-3",
  "version": "0.3.7",
  "description": "A Vue.js plugin to detect idle/non-active users",
  "scope": "VIdle3",
  "main": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eladcandroid/v-idle-3.git"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "author": "Elad Cohen <eladc.android@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/eladcandroid/v-idle-3/issues"
  },
  "homepage": "https://github.com/eladcandroid/v-idle-3#readme",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "publish": "npm run build && npm publish"
  },
  "dependencies": {
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "^2.5.1",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
